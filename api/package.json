{
  "private": true,
  "name": "twine-api",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TwinePlatform/twine-api.git"
  },
  "contributors": [
    "Elias Malik <elias@foundersandcoders.com>",
    "Aisha Nasir <aisha@foundersandcoders.com>"
  ],
  "bugs": {
    "url": "https://github.com/TwinePlatform/twine-api/issues"
  },
  "homepage": "https://github.com/TwinePlatform/twine-api#readme",
  "scripts": {
    "test:unit": "NODE_ENV=testing jest --config=jest.unit.config.js",
    "test:integration": "NODE_ENV=testing jest --config=jest.integration.config.js --runInBand",
    "test": "npm run test:unit && npm run test:integration",
    "cover:unit": "npm run test:unit -- --coverage",
    "cover:integration": "npm run test:integration -- --coverage",
    "cover": "npm run cover:unit && npm run cover:integration",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "checks": "node ./build/bin/server_setup.js",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:latest:prod": "NODE_ENV=production knex migrate:latest --knexfile knexfile.ts",
    "migrate:latest:test": "NODE_ENV=testing knex migrate:latest --knexfile knexfile.ts",
    "migrate:make": "ts-node ./bin/db.ts migrate:make",
    "migrate:teardown": "ts-node ./bin/db.ts migrate:teardown",
    "migrate:teardown:prod": "NODE_ENV=production ts-node ./bin/db.ts migrate:teardown",
    "migrate:teardown:test": "NODE_ENV=testing ts-node ./bin/db.ts migrate:teardown",
    "db:reset": "npm run migrate:teardown && npm run migrate:latest",
    "db:reset:prod": "npm run migrate:teardown:prod && npm run migrate:latest:prod",
    "db:reset:test": "npm run migrate:teardown:test && npm run migrate:latest:test",
    "seed:make": "knex seed:make --knexfile knexfile.ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts",
    "seed:run:test": "NODE_ENV=testing knex seed:run",
    "dev": "NODE_ENV=development nodemon",
    "copy:assets": "cp -R ./assets ./build/assets",
    "copy:fonts": "cp -R ./src/services/pdf/fonts ./build/src/services/pdf/fonts",
    "copy": "npm run copy:assets && npm run copy:fonts",
    "start": "node ./build/src/index.js",
    "watch": "tsc -w",
    "build": "tsc && npm run copy",
    "heroku-postbuild": "cd ../lib/twine-util && npm run build && cd ../../api && npm run build",
    "exec": "ts-node --files"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/catbox": "^10.2.1",
    "@hapi/catbox-redis": "^5.0.2",
    "@hapi/hapi": "^18.3.1",
    "@hapi/joi": "^15.1.0",
    "@hapi/yar": "^9.2.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.3.3",
    "@types/hapi-auth-jwt2": "^8.0.2",
    "@types/hapi__boom": "^7.4.0",
    "@types/hapi__catbox": "^10.2.2",
    "@types/hapi__catbox-redis": "^5.0.0",
    "@types/hapi__hapi": "^18.2.5",
    "@types/hapi__joi": "^15.0.2",
    "@types/hapi__yar": "^9.2.0",
    "@types/jest": "^24.0.15",
    "@types/minimist": "^1.2.0",
    "@types/pdfmake": "^0.1.3",
    "@types/qs": "^6.5.3",
    "@types/ramda": "0.26.18",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "chalk": "^2.4.2",
    "cookie": "^0.4.0",
    "csv": "^5.1.1",
    "dotenv": "^6.2.0",
    "hapi-auth-bearer-token": "^6.2.1",
    "hapi-pino": "^5.4.1",
    "knex": "^0.16.5",
    "moment": "^2.24.0",
    "pdfmake": "^0.1.53",
    "pg": "^7.11.0",
    "postmark": "^2.2.7",
    "qrcode": "^1.3.3",
    "qs": "^6.7.0",
    "ramda": "^0.26.1",
    "random-words": "^1.1.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "twine-util": "file:../lib/twine-util",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/mockdate": "^2.0.0",
    "@types/nock": "^9.3.1",
    "axios-mock-adapter": "^1.16.0",
    "factory-girl": "^5.0.4",
    "jest": "^24.8.0",
    "minimist": "^1.2.0",
    "mockdate": "^2.0.2",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "tslint-config-twine": "file:../lib/tslint-config-twine"
  }
}
