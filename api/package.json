{
  "private": true,
  "name": "twine-api",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "12.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TwinePlatform/twine-api.git"
  },
  "contributors": [
    "Elias Malik <elias@foundersandcoders.com>",
    "Aisha Nasir <aisha@foundersandcoders.com>"
  ],
  "bugs": {
    "url": "https://github.com/TwinePlatform/twine-api/issues"
  },
  "homepage": "https://github.com/TwinePlatform/twine-api#readme",
  "scripts": {
    "test:unit": "NODE_ENV=testing jest --config=jest.unit.config.js",
    "test:integration": "NODE_ENV=testing jest --config=jest.integration.config.js",
    "test:codeOp": "NODE_ENV=testing jest  --config=jest.integration.config.js src/api/v1/community_businesses/__tests__/get.test.integration.ts -t 'success:: definate pass'",
    "test:codeOpCider": "NODE_ENV=testing jest --config=jest.integration.config.js",
    "test": "npm run test:unit && npm run test:integration",
    "cover:unit": "npm run test:unit -- --coverage",
    "cover:integration": "npm run test:integration -- --coverage",
    "cover": "npm run cover:unit && npm run cover:integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "checks": "node ./build/bin/server_setup.js",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:latest:prod": "NODE_ENV=production knex migrate:latest --knexfile knexfile.ts",
    "migrate:latest:test": "NODE_ENV=testing knex migrate:latest --knexfile knexfile.ts",
    "migrate:latest:test-windows": "knex migrate:latest --knexfile knexfile.ts NODE_ENV=testing",
    "migrate:make": "ts-node ./bin/db.ts migrate:make",
    "migrate:teardown": "ts-node ./bin/db.ts migrate:teardown",
    "migrate:teardown:prod": "NODE_ENV=production ts-node ./bin/db.ts migrate:teardown",
    "migrate:teardown:test": "NODE_ENV=testing ts-node ./bin/db.ts migrate:teardown",
    "migrate:teardown:test-windows": "ts-node ./bin/db.ts migrate:teardown NODE_ENV=testing",
    "db:reset": "npm run migrate:teardown && npm run migrate:latest",
    "db:reset:prod": "npm run migrate:teardown:prod && npm run migrate:latest:prod",
    "db:reset:test": "npm run migrate:teardown:test && npm run migrate:latest:test",
    "db:update": "node ./database/update.js",
    "db:reset:test-windows": "npm run migrate:teardown:test-windows && npm run migrate:latest:test-windows",
    "seed:make": "knex seed:make --knexfile knexfile.ts",
    "seed:run": "knex seed:run --knexfile knexfile.ts",
    "seed:run:test": "NODE_ENV=testing knex seed:run",
    "seed:run:test-windows": "knex seed:run NODE_ENV=testing --knexfile knexfile.ts",
    "dev": "NODE_ENV=development nodemon",
    "dev-native": "NODE_ENV=development HOST_ADDRESS=0.0.0.0 nodemon",
    "dev-windows": "nodemon NODE_ENV=development",
    "dev-native-windows": "nodemon NODE_ENV=development HOST_ADDRESS=0.0.0.0",
    "copy:assets": "cp -R ./assets ./build/assets",
    "copy:fonts": "cp -R ./src/services/pdf/fonts ./build/src/services/pdf/fonts",
    "copy": "npm run copy:assets && npm run copy:fonts",
    "start": "node ./build/src/index.js",
    "watch": "tsc -w",
    "build": "tsc && npm run copy",
    "heroku-postbuild": "cd ../lib/twine-util && npm i && npm run build && cd ../../api && npm run build",
    "exec": "ts-node --files",
    "redis:enable-notify": "npm run exec ./database/redis/configure.ts"
  },
  "dependencies": {
    "@hapi/boom": "^9.0.0",
    "@hapi/catbox": "^10.2.1",
    "@hapi/catbox-redis": "^5.0.2",
    "@hapi/hapi": "^20.0.0",
    "@hapi/joi": "^15.1.0",
    "@hapi/mimos": "^5.0.0",
    "@hapi/validate": "^1.1.2",
    "@hapi/yar": "^9.2.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.3.3",
    "@types/hapi-auth-jwt2": "^8.0.2",
    "@types/hapi__boom": "^9.0.0",
    "@types/hapi__catbox": "^10.2.2",
    "@types/hapi__catbox-redis": "^5.0.0",
    "@types/hapi__hapi": "20.0.0",
    "@types/hapi__mimos": "4.1.0",
    "@types/hapi__joi": "^15.0.2",
    "@types/hapi__yar": "^9.2.0",
    "@types/jest": "^24.0.15",
    "@types/mime-db": "1.27.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.6",
    "@types/pdfmake": "0.1.3",
    "@types/qs": "^6.5.3",
    "@types/ramda": "^0.27.11",
    "@types/ua-parser-js": "^0.7.33",
    "axios": "^0.19.0",
    "bcrypt": "^5.0.0",
    "chalk": "^2.4.2",
    "cookie": "^0.4.0",
    "csv": "^5.1.1",
    "dotenv": "^6.2.0",
    "hapi-auth-bearer-token": "^6.2.1",
    "hapi-pino": "^8.0.1",
    "ioredis": "4.14.0",
    "knex": "^0.16.5",
    "moment": "^2.24.0",
    "neat-csv": "^5.2.0",
    "pdfmake": "0.1.53",
    "pg": "^7.11.0",
    "postmark": "^2.2.7",
    "qrcode": "^1.3.3",
    "qs": "^6.7.0",
    "ramda": "^0.27.0",
    "ts-node": "^8.10.2",
    "twine-util": "file:../lib/twine-util",
    "typescript": "^3.8.3",
    "ua-parser-js": "^0.7.20"
  },
  "devDependencies": {
    "@types/mockdate": "^2.0.0",
    "@types/nock": "^9.3.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "axios-mock-adapter": "^1.16.0",
    "eslint": "^6.2.2",
    "eslint-config-twine": "file:../lib/eslint-config-twine",
    "factory-girl": "^5.0.4",
    "jest": "^24.8.0",
    "minimist": "^1.2.0",
    "mockdate": "^2.0.2",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.9.0"
  }
}
